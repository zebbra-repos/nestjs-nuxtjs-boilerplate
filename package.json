{
  "name": "nest-nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Application boilerplate for NestJS and NuxtJS",
  "author": {
    "name": "Michael BÃ¤rtschi"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "deps": "yarn upgrade-interactive --latest",
    "prepare": "husky install",
    "postinstall": "node scripts/vue-apollo-patch.js",
    "prebuild": "rimraf dist .nuxt && yarn generate:types",
    "postbuild": "yarn copy",
    "build": "npm-run-all -p build:**",
    "build:server": "nest build",
    "build:client": "nuxt build",
    "pretypeorm": "(rm ormconfig.json || :) && CLI_PATH=app/server/src/commands/cli.ts yarn nestjs-command create:ormconfig",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration:generate": "yarn typeorm migration:generate -n",
    "db:migrate": "yarn typeorm migration:run",
    "db:rollback": "yarn typeorm migration:revert",
    "lint": "npm-run-all -s lint:**",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:pug": "pug-lint-vue ./app/client",
    "start:prod": "NODE_ENV=production node dist/app/server/src/main",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "start:client:dev": "nuxt",
    "start:client": "nuxt start",
    "test": "jest",
    "test:backend": "jest --config ./app/server/jest.backend.config.js",
    "test:unit": "jest --config ./app/server/jest-unit.config.js",
    "test:unit:cov": "jest --config ./app/server/jest-unit.config.js --runInBand --coverage",
    "test:e2e": "jest --config ./app/server/test/jest-e2e.config.js",
    "test:e2e:cov": "jest --config ./app/server/test/jest-e2e.config.js --runInBand --coverage",
    "test:client": "jest --config ./app/client/jest-client.config.js",
    "test:client:cov": "jest --config ./app/client/jest-client.config.js --runInBand --coverage",
    "generate:types": "graphql-codegen --config codegen.yml",
    "copy": "npm-run-all -p copy:**",
    "copy:nest:locales": "cp -r ./app/server/locales ./dist/app/server",
    "copy:nest:templates": "cp -r ./app/server/templates ./dist/app/server",
    "copy:nuxt:locales": "cp -r ./app/client/locales ./dist/app/client/dist",
    "copy:nuxt:static": "cp -r ./app/client/static ./dist/app/client/dist"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@ntegral/nestjs-sentry": "^3.0.3",
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/composition-api": "^0.26.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.12.1",
    "@sentry/browser": "^6.10.0",
    "@sentry/integrations": "^6.10.0",
    "@sentry/node": "^6.10.0",
    "@vue/apollo-composable": "^4.0.0-alpha.14",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-typeorm": "^1.1.4",
    "consola": "^2.15.3",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "csurf": "^1.11.0",
    "express-session": "^1.17.2",
    "flat": "^5.0.2",
    "graphql": "^15.5.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "nestjs-command": "^2.0.1",
    "nestjs-i18n": "^8.1.3",
    "nestjs-pino": "^1.4.0",
    "nodemailer": "^6.6.3",
    "nodemailer-sendgrid": "^1.0.3",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "6.21.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pino-pretty": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "typeorm": "^0.2.36",
    "typeorm-static": "^1.1.1",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@graphql-codegen/typescript-vue-apollo": "^3.0.0",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/vue-app": "^2.15.7",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.7",
    "@types/flat": "^5.0.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.11",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-sendgrid": "^1.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@vue/test-utils": "^1.2.2",
    "apollo-client": "^2.6.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "pug": "^3.0.2",
    "pug-lint-vue": "^0.4.0",
    "pug-plain-loader": "^1.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^22.0.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "8.3.0",
    "ts-node": "^10.1.0",
    "typeorm-factories": "^1.0.1",
    "typescript": "^4.3.5",
    "value-or-promise": "^1.0.10",
    "vue-jest": "^3.0.7",
    "vuex": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint",
    "*.{vue}": "pug-lint-vue"
  }
}
